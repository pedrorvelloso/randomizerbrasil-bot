{
  "name": "rbr-bot",
  "version": "1.6.1",
  "description": "Discord bot for managing runners/streamers in Supabase",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "register": "tsx src/register-commands.ts",
    "clear": "tsx src/clear-commands.ts",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src"
  },
  "keywords": [
    "discord",
    "bot",
    "twitch",
    "supabase"
  ],
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "discord.js": "^14.16.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.5.0"
  }
}
